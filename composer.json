{
    "name": "greenleafadvancement/postmark",
    "description": "Sync WP content from Markdown files (CommonMark v2 compatible fork)",
    "type": "wp-cli-package",
    "homepage": "https://github.com/greenleafadvancement/postmark/",
    "license": "MIT",
    "authors": [
        {
            "name": "PJ Eby",
            "homepage": "https://github.com/pjeby"
        },
        {
            "name": "Greenleaf Advancement",
            "homepage": "https://greenleafadvancement.com"
        }
    ],
    "keywords": [
        "markdown",
        "wordpress",
        "wp",
        "wp-cli",
        "yaml",
        "front-matter",
        "content-management",
        "configuration-management",
        "static-site-generator",
        "yaml front matter"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [ "command.php" ],
        "psr-4": {
            "dirtsimple\\Postmark\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "greenleafadvancement/clean-yaml": "dev-master",
        "dirtsimple/imposer": "dev-master",
        "wp-cli/wp-cli": "^2.11",
        "wp-cli/entity-command": "^2.8",
        "rarst/wpdatetime": "^0.3.0",
        "league/commonmark": "^2.7",
        "twig/twig": "^3.0",
        "symfony/yaml": "^5.4 || ^6.0 || ^7.0"
    }
}
